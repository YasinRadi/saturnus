let { entries, `+?` } = use std;

fn `|>`(left, right) {
  return right(left);
}

fn `<|`(left, right) {
  return left(right);
}

// Example usage of extra operators:
let r = 101
  |> (a => a + 10)
  |> (a => a * 2)
  |> tostring;

let kind = type <| r |> a => "(" ++ a ++ ")";

print("Result = " ++ r ++ " " ++ kind);
print("Maybe sum", () +? 10);
print("Maybe sum", 10 +? 10);
