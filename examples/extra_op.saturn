let { operator, entries } = use std;

@operator("|>")
fn pipe_right(left, right) {
  return right(left);
}

@operator("<|")
fn pipe_left(left, right) {
  return left(right);
}

// Example usage of extra operators:
let r = 101
  |> (a => a + 10)
  |> (a => a * 2)
  |> tostring;

let kind = type <| r |> a => "(" ++ a ++ ")";

print("Result = " ++ r ++ " " ++ kind);
