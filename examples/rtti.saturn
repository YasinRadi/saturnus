// Example of runtime type information
use { rtti: { arguments } } in std;

class Foo {
  @arguments(("self", "any"), ("num", "number"), ("str", "string"))
  fn bar(self, num, str) {
    return "1 + " ++ num ++ " = " ++ (1 + num) ++ str;
  }
}

let foo = Foo {};

if foo is Foo {
  print("foo is Foo!");
}

let typeOfFoo = typeof foo;
print("Is type of foo?", typeOfFoo == Foo);

// Runtime type checks
print(foo->bar(10, "!"));
// this will blow:
print(foo->bar("a", 99));
